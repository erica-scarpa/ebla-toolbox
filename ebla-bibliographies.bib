@article{Pomata1994,
  title = {Legami Di Sangue, Legami Di Seme. {{Consanguineità}} e Agnazione Nel Diritto Romano},
  author = {Pomata, Gianna},
  date = {1994},
  journaltitle = {Quaderni storici},
  volume = {29},
  pages = {299--334},
  issn = {0301-6307},
  abstract = {In European history, the notion of consanguinity has been treated as self-evident and as generally meaning — as it does for us — a blood tie created through either the mother or the father. However, this has not always been the case. The concept of consanguinity we find in Roman law is very different from the modern one or even from the one to be found in later times in medieval Canon law. In Roman legal texts the term consanguinitas is used to refer exclusively to the tie derived from the paternal blood. Only the brothers and sisters born from the same father are called consanguinei (that is, sharing the same blood), even if they don't have the same mother. Children with the same mother but different fathers are not called consanguinei and don't have the legal rights stemming from consanguinitas. Thus the Roman notion of consanguinity is strictly associated with the concept of "agnation" (that is to say, kinship created only through males). This paper tries to understand this notion by placing it in the context of ancient medical theories on blood, semen and gender.},
  issue = {86 (2)},
  file = {/Users/ericascarpa/Bookshelf/Pomata1994 - Legami di sangue, legami di seme - Quaderni storici 29 - N0650.pdf}
}

