@inproceedings{Fronzaroli2003b-h,
  title = {Various {{Cuneiform Texts}}},
  shorttitle = {Art of the {{First Cities}}},
  booktitle = {Art of the {{First Cities}}: {{The Third Millennium B}}.{{C}}. from the {{Mediterranean}} to the {{Indus}}},
