@article{Fronzaroli2012,
  title = {The Eblaic King's Supplication to the Gods of the Night (TM.75.G.756+771+815)},
  author = {Fronzaroli, Pelio},
  date = {2012},
  journaltitle = {RA},
  volume = {106},
  eprint = {42771752},
  eprinttype = {jstor},
  pages = {165--176},
  publisher = {{Presses Universitaires de France}},
  issn = {0373-6032},
  abstract = {In this article I attempt to improve the interpretation of the Eblaic text TM.75.G.756+ (published by Biga 2003), with a new examination. The text was originally divided into three parts separated by blanks. Subsequently, additions were traced on the dry clay in the cases initially left unwritten. The first part lists offerings probably intended to be placed in front of seven thrones for the astral divinities invoked in the rite (dGi₆-an). In this same part a black cloak for the kallatum dGi₆-an and seven women covered by a black cloak are mentioned. The first addition seems to belong to the textlinguistics ("Tablet of the (procedures) established"). The second part lists offerings to Gir(r)a (dNE-ra) and his companion Hapas (dA-ba-sa) "(Counter) witchcraft". The second addition, a self-standing section, indicates the number of the participants in the rite. The third part is a colophon indicating the occasion of the ritual ("in the occasion of the supplication of the king"). The tablet, as the administrative texts found in the same room L. 2712, must have been written in a period immediately preceding the destruction of the Palace G. The gods of the night are mentioned only in one other Eblaic document, an administrative text datable to the last period of the Archives. The ritual was probably introduced into Ebla in a very late period. Dans cet article, je tente d’améliorer l’interprétation du texte éblaique TM.75,G756+ (publié par Biga en 2003), suite à un nouvel examen. Le texte était originairement divisé en trois parties séparées par des blancs. Ultérieurement, des additions furent tracées sur l’argile sèche dans des cases laissées initialement non écrites. La première partie énumère des offrandes qu’on prévoyait probablement de placer devant les sept trônes des divinités astrales invoquées dans le rite (dGi₆-an). Dans la même partie, une cape noire pour la kallatum dGi₆-an et sept femmes couvertes par une cape noire sont mentionnées. La première addition semble appartenir au texte lui-même (“Tablettes des (procédures) établies”). La seconde partie énumère des offrandes à Gir(r)a (dNE-ra) et à son compagnon Hapas (da-ba-sa) ”(Contre) sorcellerie”. La seconde addition, une section indépendante, indique le nombre de participants au rite. La troisième partie est le colophon précisant l’occasion du rituel (“à l’occasion de la supplication du roi”). La tablette, ainsi que les textes administratifs trouvés dans la même pièce L.2712, ont dû être rédigés dans la période précédant immédiatement la destruction du palais G. Les dieux de la nuit sont seulement mentionnés dans un autre document éblaitique, un texte administratif datable de la dernière période des Archives. Le rituel a été probablement introduit à Ébla à une période tardive.},
  langid = {french},
  keywords = {Ebla Tablets: Joins; Editions; and Discussions},
  file = {/Users/ericascarpa/Bookshelf/Fronzaroli2012 - The Eblaic King's Supplication to the Gods of the Night - RA 106 - EB1052.pdf}
}

