@article{HazellEtAl2022,
  title = {High-{{Resolution Bronze Age Palaeoenvironmental Change}} in the {{Eastern Mediterranean}}: {{Exploring}} the {{Links Between Climate}} and {{Societies}}},
  shorttitle = {High-{{Resolution Bronze Age Palaeoenvironmental Change}} in the {{Eastern Mediterranean}}},
