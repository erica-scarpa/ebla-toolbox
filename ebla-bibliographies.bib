@article{Markovic1972,
  title = {The {{Problem}} of {{Reification}} and the {{Verstehen-Erklären Controversy}}},
  author = {Marković, Mihailo},
  date = {1972},
  journaltitle = {Acta Sociol.},
  volume = {15},
  number = {1},
  eprint = {4193854},
  eprinttype = {jstor},
  pages = {27--38},
  issn = {0001-6993},
  abstract = {The dilemma Erklären -- Verstehen cannot be resolved either by simply rejecting one of the alternative methods, or by relegating them to two different fields of application (sciences versus history and philosophy). Explanation and understanding are two opposite but complementary dimensions of social inquiry. The emphasis on one or the other will depend (1) on the degree of reification of a particular social situation to be studied, and (2) on the extent to which we need an objective, validated account of the situation. A social situation is reified if its actors acquire, or are a scribed, the characteristics of things. The concept of reification is applied to three different levels: (i) objects (human behaviour under certain historical conditions actually obtains thinglike properties: work becomes repetitive, political life formalistic, society is ruled by alienated social forces); (ii) theory about objects (in the search for objective knowledge individual, subjective features of human behaviour must be left aside); (iii) meta-theory (when critical consciousness about the limits of our objective methodology is absent and when our intellectual instruments become too frozen and hardened). The greater the degree of reification in the field of our study, the more adequate will be the methods of external observation and theoretical explanation analogous with those in natural sciences. No matter how strongly motivated isolated individuals might be, unpredictable interactions among them (in a poorly organised society) acquire the character of chance events. Central statistical tendencies of these appear as blind forces that prevail over individual human wills. Therefore understanding of individual motives and aspirations plays a minor cognitive role under these conditions. However, in those fields of social activity where individual values and goals are permanently relevant (eg. culture) or in those phases of history where conscious collective coordination becomes decisive (eg. in epochs of social change) the study of overt behaviour does not suffice and must be supplemented by the study of human needs, drives and intentions. Furthermore, while the method of understanding is considerably inferior from the point of view of intersubjective reliability and validity of its result there are situations in which we either do not need or cannot have objective knowledge, and in which precisely individual and group understanding is indispensable (ie. when we have to act quickly, when the personnel and funds for objective scientific investigation are lacking and when we are primarily interested in free, spontaneous individual- or group-behaviour). It is misleading to construe methods of explanation and understanding as two polar opposites (the former being purely objective, the later simply subjective). On the one hand, the choice of problems to be investigated, the selection and interpretation of data, the direction of analysis and theory building will inevitably be influenced by the scholar's scale of value and various extra-scientific considerations. On the other hand, the process of internalisation of observed data need not be purely subjective (in so far as there might be a high degree of correlation among introspective statements of various individuals). Also the statements that serve to connect two subjective states (eg. thought and a goal) may be well supported by the theoretical findings of psychology. In this case the method of understanding coincides with a specific (motivational) type of the method of explanation. At the end the author shows how Erklären and Verstehen may be reinterpreted as two special moments within dialectic, conceived as a method of critical social study characterised by both structural and historical, objective and subjective approach.},
  file = {/Users/ericascarpa/Bookshelf/Markovic1972 - The Problem of Reification and the Verstehen-Erklären Controversy - Acta Sociol. 15 - N0428.pdf}
}

