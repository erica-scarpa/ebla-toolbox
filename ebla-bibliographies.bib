@thesis{Scarpa2020a,
  type = {phdthesis},
  title = {Social {{History Through Textual Patterns}}: {{Study}} on the {{Social Organization}} at {{Ebla During}} the {{Age}} of the {{Archives}} ({{Syria}}, 24th {{Century Bce}})},
  shorttitle = {Social {{History Through Textual Patterns}}},
  author = {Scarpa, Erica},
  date = {2020},
  institution = {{Ca' Foscari University of Venice}},
  url = {http://dspace.unive.it/handle/10579/17824},
  urldate = {2022-05-02},
  abstract = {I testi cuneiformi degli Archivi Reali di Ebla (Siria, XXIV sec a.C.) testimoniano 40 anni della vita economica, sociale e politica della città. La maggior parte dei documenti conservati all’interno degli Archivi sono di carattere economico, amministrativo, e diplomatico e offrono informazioni preziose per la ricostruzione della storia politica ed economica della città. A questi si aggiungono, in numero assai inferiore, testi letterari (incantesimi, inni) e lessicali (testi lessicali monoligui e bilingui). L’obiettivo della ricerca è proporre un modello della società eblaita basato sui dati presenti nella documentazione degli Archivi Reali: il presente studio utilizza quindi come parole chiave i termini che nei testi di Ebla contraddistinguono le categorie professionali, al fine di delineare un modello che permetta di comprendere i livelli di interazione e la gerarchia che caratterizzava le componenti della società eblaita attraverso una metodologia di matrice sociologica e storico-sociale. Ciascun termine chiave è analizzato in chiave lessicale, amministrativa, e storica, al fine di delineare le principali componenti del tessuto sociale: l’analisi dei dati così raccolti mira a delineare un quadro generale dell’organizzazione sociale di Ebla alla metà del III millennio a.C..},
  keywords = {Society at Ebla},
  annotation = {Accepted: 2019-12-09},
  file = {/Users/ericascarpa/Bookshelf/Scarpa2020 - Social history through textual patterns - EB2902.pdf}
}

