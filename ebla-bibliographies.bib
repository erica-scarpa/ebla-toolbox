@article{AnderssonStrand2012,
  title = {The textile \emph{chaîne opératoire}: Using a multidisciplinary approach to textile archaeology with a focus on the Ancient Near East},
  shorttitle = {The textile chaîne opératoire},
  author = {Andersson Strand, Eva B.},
  date = {2012},
  journaltitle = {Paléorient},
  volume = {38},
  number = {1},
  pages = {21--40},
  doi = {10.3406/paleo.2012.5456},
  abstract = {Les connaissances sur l’histoire du textile (fibres, technologie, outils, etc.) sont fondamentales pour notre compréhension du passé. La recherche sur le textile est devenue un secteur important de l’archéologie et présente un énorme potentiel, susceptible renseigner sur l’économie, la chronologie et différents aspects culturels des sociétés du passé. En raison des conditions taphonomiques, très peu de textiles ont survécu au Proche-Orient. Néanmoins, les quelques fragments préservés, combinés avec d’autres sources, suggèrent un savoir-faire développé quant à l’exploitation des fibres. Les analyses montrent en outre l’utilisation textile de plusieurs types de plantes et de fibres animales. Le lin et la laine passent pour être les plus importantes fibres textiles, du Néolithique aux temps modernes. Les analyses suggèrent en outre que les différentes étapes du travail des fibres étaient semblables dans toute l’Eurasie, bien que les différentes zones climatiques aient eu une incidence sur les besoins et les accès aux fibres. L’archéobotanique et l’archéozoologie informent également sur l’usage des textiles. Différents types d’installations (fosses, ateliers, etc.) et d’outils textiles (maillets, fusaïoles, métiers à tisser) ont existé et, bien que la plupart d’entre eux aient disparu du fait de leur matériau périssable, des fusaïoles et des pesons de métier sont bien attestés dans les contextes archéologiques. Cet article propose un cadre de base pour comprendre la production textile et apporte un éclairage sur l’obtention et le travail des fibres végétales et animales. On abordera plus brièvement la question du filage et du tissage. Pour cela, des informations issues de sources archéologiques et écrites seront combinées avec l’ethnographie et l’archéologie expérimentale.},
  langid = {french},
  keywords = {Fashionable Outfits},
  file = {/Users/ericascarpa/Bookshelf/AnderssonStrand2012 - The textile chaîne opératoire - Paléorient 38.pdf}
}

