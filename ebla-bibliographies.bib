@article{BolandPondy1983,
  title = {Accounting in {{Organizations}}: {{A Union}} of {{Natural}} and {{Rational Perspectives}}},
  author = {Boland, Richard J. and Pondy, Louis R.},
  date = {1983},
  journaltitle = {Account. Org. Soc.},
  volume = {8},
  number = {2},
  pages = {223--234},
  issn = {0361-3682},
  doi = {10.1016/0361-3682(83)90030-2},
  abstract = {Research on accounting in its organizational context is most fruitfully done by attempting to understand how its rational and natural aspects interact within the lived experience of individuals. Accounting serves both objective and symbolic functions. Research that emphasizes a genuine union of the two aspects reveals accounting's role as a complement and supplement to more qualitative and interactive forms of problem solving. It also reveals that accounting is a technique that must be transcended to be used effectively and that its inadequacies challenge humans as moral agents.}
}

